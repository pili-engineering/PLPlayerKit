{
  "name": "PLPlayerKit",
  "version": "3.4.6",
  "summary": "Pili iOS video player SDK, RTMP, HLS video streaming supported.",
  "homepage": "https://github.com/pili-engineering/PLPlayerKit",
  "license": "Apache License, Version 2.0",
  "authors": {
    "pili": "pili-coresdk@qiniu.com"
  },
  "source": {
    "http": "https://sdk-release.qnsdk.com/PLPlayerKit-iphoneos-v3.4.6.zip"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "frameworks": [
    "UIKit",
    "Foundation",
    "MediaPlayer",
    "CoreAudio",
    "AudioToolbox",
    "Accelerate",
    "QuartzCore",
    "OpenGLES",
    "AVFoundation",
    "CoreVideo",
    "AVKit",
    "CoreMedia",
    "VideoToolbox",
    "CoreTelephony"
  ],
  "libraries": [
    "c++",
    "z",
    "bz2",
    "iconv",
    "resolv"
  ],
  "subspecs": [
    {
      "name": "iphoneos",
      "vendored_frameworks": "Pod/Library/PLPlayerKit.framework"
    }
  ]
}
